{
  "name": "@panenco/shortlist-ui",
  "version": "1.0.91",
  "description": "shortlist ui kit",
  "module": "lib/ui.esm.js",
  "main": "lib/ui.cjs.js",
  "license": "UNLICENSED",
  "repository": "git@github.com:Panenco/myshortlist-ui.git",
  "scripts": {
    "start": "start-storybook -p 9001 -c .storybook",
    "build": "npm run clean && cross-env BABEL_ENV=production rollup --config rollup.config.js",
    "build:storybook": "build-storybook -c .storybook -o storybook-dist",
    "watch": "npm run clean && rollup --config rollup.config.js --watch",
    "clean": "rimraf lib",
    "eslint": "eslint config src stories  --ext .js --ext .jsx",
    "prettier": "prettier '{config,src,stories}/**/*.{js,jsx}' --write",
    "stylelint": "stylelint 'src/**/*.scss'",
    "pre-commit": "lint-staged",
    "prepublishOnly": "npm run build"
  },
  "sideEffects": [
    "**.css"
  ],
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run pre-commit"
    }
  },
  "peerDependencies": {
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
